{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "api/app.py"
        },
        {
            "src": "api/(.*)",
            "headers": {"Access-Control-Allow-Origin": "https://blog-teal-three-22.vercel.app"},
            "continue": true
        }
    ]
}