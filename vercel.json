{
    "version": 2,
    "builds": [
        {
            "src": "./api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api"
        }
    ],
    "buildCommand": "flask run",
    "installCommand": "pip install -r requirements.txt",
    "outputDirectory": "/api/templates"
}